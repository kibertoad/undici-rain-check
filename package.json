{
  "name": "undici-rain-check",
  "version": "1.0.0",
  "author": "Igor Savin <kibertoad@gmail.com>",
  "main": "dist/undici-rain-check",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:release": "del-cli dist && del-cli coverage && npm run lint && npm run build",
    "test": "vitest",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint .  --ext .ts --fix",
    "format": "prettier --write .",
    "docker:start": "docker-compose -f docker-compose.yml up --build -d redis && docker-compose -f docker-compose.yml up --build -d wait_for_redis",
    "docker:stop": "docker-compose -f docker-compose.yml down",
    "prepublishOnly": "npm run build:release"
  },
  "peerDependencies": {
    "undici-retry": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitest/coverage-c8": "^0.31.4",
    "del-cli": "^5.0.0",
    "ioredis": "^5.3.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vitest": "^0.2.5",
    "mockttp": "^3.7.5",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "undici": "^5.22.1",
    "undici-retry": "^1.1.1",
    "vitest": "^0.31.4"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/undici-rain-check.git"
  },
  "keywords": [
    "http",
    "retry",
    "undici",
    "guaranteed",
    "delivery",
    "redis"
  ],
  "homepage": "https://github.com/kibertoad/undici-rain-check",
  "files": [
    "README.md",
    "LICENSE",
    "dist/*"
  ]
}
